{
  "name": "dump",
  "packageManager": "yarn@3.1.1",
  "type": "module",
  "scripts": {
    "watch:server": "rm -rf dist && tsc-watch --onSuccess 'yarn run start:server'",
    "start:server": "yarn run copy:files && node --inspect=0.0.0.0:9229 dist/service.js",
    "copy:files": "copyfiles -u 1 src/**/*.ejs dist"
  },
  "dependencies": {
    "@daisugi/kado": "^0.1.10",
    "@daisugi/kintsugi": "^0.3.9",
    "ejs": "^3.1.6",
    "fastify": "^3.25.2",
    "fastify-compress": "^4.0.1",
    "fastify-formbody": "^5.2.0",
    "fastify-static": "^4.5.0",
    "joi": "^17.5.0",
    "knex": "^0.95.15",
    "pg": "^8.7.1",
    "point-of-view": "^5.0.0"
  },
  "devDependencies": {
    "@types/ejs": "^3.1.0",
    "@types/node": "^17.0.5",
    "@typescript-eslint/eslint-plugin": "^5.8.1",
    "@typescript-eslint/parser": "^5.8.1",
    "copyfiles": "^2.4.1",
    "eslint": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "prettier": "^2.5.1",
    "tsc-watch": "^4.6.0",
    "typescript": "^4.5.4"
  }
}
