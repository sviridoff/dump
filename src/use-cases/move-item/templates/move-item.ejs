<%- include('../../templates/header'); -%>

<h1>
  <%= user.username %> / <%= item.title %> (<%=
  item.visibility %>)
</h1>

<h2>Move item</h2>

<% if (items.length){ %>
<ul>
  <% items.forEach(function(item) { %>
  <li>
    <a href="/<%= user.username %>/<%= item.slug %>/post"
      ><%= item.title %> (<%= item.visibility %>)</a
    >
  </li>
  <% }); %>
</ul>

<% } %> <%- include('../../templates/footer'); -%>
