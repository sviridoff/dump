<%- include('../../templates/header'); -%>

<h1><%= user.username %>/<%= item.name %></h1>
<% if (item.items.length){ %>
  <ul>
    <% item.items.forEach(function(item) { %>
      <li>
        <a href="/<%= user.username %>/<%= item.slug %>"><%= item.name %></a>
      </li>
    <% }); %>
  </ul>
<% } %>

<a href="/<%= user.username %>/<%= item.slug %>/new">New</a>

<%- include('../../templates/footer'); -%>